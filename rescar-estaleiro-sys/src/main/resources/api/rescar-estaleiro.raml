title: RESCAR - Acesso
baseUri: http://localhost:8088
(amf-serverDescription): Generated server url
description: API de serviços
version: Versão 1.0
(amf-contact):
  name: rodrigo.wisnievski@serpro.gov.br
documentation:
  -
    content: Arquitetura de Referência Backend (DDD+Hexagonal)
    title: https://dedat.gitpages.serpro/deat9/arquitetura-referencia-backend-ddd/
/v1/usuarios/{id}:
  get:
    displayName: find
    description: Obter Usuario por id
    (amf-tags):
      - usuario-resource
    (amf-baseUriParameters):
      id:
        required: true
        type: string
    responses:
      "200":
        description: OK
        body:
          "*/*": UsuarioRepresentation
    securedBy:
      - Authorization
  put:
    displayName: update
    description: Alterar Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: AlterarDadosPessoaisCommand
    (amf-baseUriParameters):
      id:
        required: true
        type: string
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
/v1/usuarios/{id}/situacao:
  put:
    displayName: updateSituacao
    description: Alterar Situacao Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: AlterarSituacaoUsuarioCommand
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
  uriParameters:
    id:
      type: string
/v1/usuarios/{id}/perfil:
  put:
    displayName: updatePerfil
    description: Alterar Perfil Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: AlterarPerfilUsuarioCommand
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
  uriParameters:
    id:
      type: string
/v1/usuarios/{id}/endereco:
  put:
    displayName: updateEndereco
    description: Alterar Endereco Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: AlterarEnderecoUsuarioCommand
    (amf-baseUriParameters):
      id:
        required: true
        type: string
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
  post:
    displayName: incluirEndereco
    description: Incluir Endereco Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: IncluirEnderecoUsuarioCommand
    (amf-baseUriParameters):
      id:
        required: true
        type: string
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
  delete:
    displayName: removeEndereco
    description: Apagar Endereco Usuario
    (amf-tags):
      - usuario-resource
    (amf-baseUriParameters):
      id:
        required: true
        type: string
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
/v1/usuarios:
  get:
    displayName: findAll
    description: Listar usuarios
    (amf-tags):
      - usuario-resource
    queryParameters:
      perfil:
        required: false
        type: string
    responses:
      "200":
        description: OK
        body:
          "*/*":
            type: array
            items: UsuarioRepresentation
    securedBy:
      - Authorization
  post:
    displayName: insert
    description: Incluir Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: IncluirUsuarioCommand
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization
/v1/usuarios/registro:
  post:
    displayName: register
    description: Registrar Usuario
    (amf-tags):
      - usuario-resource
    body:
      application/json: RegistrarUsuarioCommand
    responses:
      "200":
        description: OK
/login:
  post:
    displayName: fakeLogin
    (amf-tags):
      - auth-resource
    body:
      application/json: EfetuarLoginCommand
    responses:
      "200":
        description: OK
/v1/cep:
  get:
    displayName: find_1
    description: Busca dados de endereço pelo CEP em api externa
    (amf-tags):
      - cep-resource
    queryParameters:
      cep:
        required: false
        type: string
    responses:
      "200":
        description: OK
        body:
          "*/*": EnderecoRepresentation
    securedBy:
      - Authorization
/auth/refresh_token:
  get:
    displayName: refreshToken
    (amf-tags):
      - auth-resource
    responses:
      "200":
        description: OK
    securedBy:
      - Authorization

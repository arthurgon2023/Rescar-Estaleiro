#%RAML 1.0
types:
  AlterarEnderecoCommand:
    type: object
    properties:
      cep?:
        type: string
      logradouro?:
        type: string
      numero?:
        type: string
      complemento?:
        type: string
      bairro?:
        type: string
      localidade?:
        type: string
      uf?:
        type: string
  UsuarioRepresentation:
    type: object
    properties:
      id?:
        type: string
      email?:
        type: string
      nome?:
        type: string
      perfil?:
        type: string
      situacao?:
        type: string
      endereco?: EnderecoRepresentation
  IncluirEnderecoCommand:
    type: object
    properties:
      cep?:
        type: string
      logradouro?:
        type: string
      numero?:
        type: string
      complemento?:
        type: string
      bairro?:
        type: string
      localidade?:
        type: string
      uf?:
        type: string
  IncluirUsuarioCommand:
    type: object
    properties:
      email:
        type: string
        minLength: 5
        maxLength: 30
        required: true
      senha:
        type: string
        minLength: 6
        maxLength: 128
        required: true
      nome:
        type: string
        minLength: 3
        maxLength: 80
        required: true
      perfil:
        type: string
        required: true
      endereco?: IncluirEnderecoCommand
  AlterarEnderecoUsuarioCommand:
    type: object
    properties:
      endereco?: AlterarEnderecoCommand
  EnderecoRepresentation:
    type: object
    properties:
      id?:
        type: string
      cep?:
        type: string
      logradouro?:
        type: string
      complemento?:
        type: string
      numero?:
        type: string
      bairro?:
        type: string
      localidade?:
        type: string
      uf?:
        type: string
  EfetuarLoginCommand:
    type: object
    properties:
      email?:
        type: string
      senha?:
        type: string
  AlterarSituacaoUsuarioCommand:
    type: object
    properties:
      situacao:
        type: string
        required: true
  AlterarPerfilUsuarioCommand:
    type: object
    properties:
      perfil:
        type: string
        required: true
  AlterarDadosPessoaisCommand:
    type: object
    properties:
      email:
        type: string
        minLength: 5
        maxLength: 30
        required: true
      senha:
        type: string
        minLength: 6
        maxLength: 128
        required: true
      nome:
        type: string
        minLength: 3
        maxLength: 80
        required: true
  IncluirEnderecoUsuarioCommand:
    type: object
    properties:
      endereco?: IncluirEnderecoCommand
  RegistrarUsuarioCommand:
    type: object
    properties:
      email:
        type: string
        minLength: 5
        maxLength: 30
        required: true
      senha:
        type: string
        minLength: 6
        maxLength: 128
        required: true
      nome:
        type: string
        minLength: 3
        maxLength: 80
        required: true
      perfil:
        type: string
        required: true
      endereco?: IncluirEnderecoCommand
